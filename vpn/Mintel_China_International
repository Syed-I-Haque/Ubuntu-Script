[connection]
id=Mintel_China_International
permissions=user:$USERNAME:;
type=vpn
uuid=$UUID_CHINA_INTERNATIONAL
zone=work

[vpn]
ca=$MINTEL_VPN_DIR/ca.crt
tls-cipher=DEFAULT:@SECLEVEL=0
cert=$MINTEL_VPN_DIR/$HOSTNAME.crt
cipher=DES-EDE3-CBC
comp-lzo=adaptive
connection-type=password-tls
dev=tun
key=$MINTEL_VPN_DIR/$HOSTNAME.key
ns-cert-type=server
password-flags=1
proto-tcp=yes
remote-random=yes
remote=relay3.cn.mintel.com:443 relay-vpn1.apac.mintel.com:443
service-type=org.freedesktop.NetworkManager.openvpn
username=$MINTEL_EMAIL

[ipv4]
dns-search=mintel.ad;mintel.co.uk
method=auto
never-default=true

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=auto
never-default=true
