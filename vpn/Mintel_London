[connection]
id=Mintel_London
permissions=user:$USERNAME:;
type=vpn
uuid=$UUID_LONDON_VPN
zone=work

[vpn]
ca=$MINTEL_VPN_DIR/ca.crt
cert=$MINTEL_VPN_DIR/$HOSTNAME.crt
cipher=DES-EDE3-CBC
comp-lzo=adaptive
connection-type=password-tls
dev=tun
key=$MINTEL_VPN_DIR/$HOSTNAME.key
ns-cert-type=server
password-flags=1
proto-tcp=yes
remote-random=yes
remote=relay-vpn1.uk.mintel.com:443 relay-vpn2.uk.mintel.com:443
service-type=org.freedesktop.NetworkManager.openvpn
username=$MINTEL_EMAIL

[ipv4]
dns-search=mintel.ad;mintel.co.uk
method=auto
never-default=true

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=auto
never-default=true
