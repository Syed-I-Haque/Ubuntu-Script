[connection]
autoconnect-priority=1
id=Mintel_Chicago_wifi
permissions=user:$USERNAME:;
type=wifi
uuid=$UUID_CHICAGO
zone=work

[wifi]
mode=infrastructure
ssid=Mintel

[wifi-security]
key-mgmt=wpa-eap

[802-1x]
eap=peap;
identity=MINTEL_$MINTEL_OFFICE\\$MINTEL_NAME
password=$WINDOWS_PASSWORD
phase2-auth=mschapv2

[ipv4]
addr-gen-mode=stable-privacy
method=auto

[ipv6]
addr-gen-mode=stable-privacy
method=auto
